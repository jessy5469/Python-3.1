#NUMBER GUESSSING GAME
winning_number = 20
user_input = input("Guess a number (1-100): ")
user_input = int(user_input)
if user_input == winning_number:
    print("YOU WIN !!!")
    
else: # nested if else
    if user_input < winning_number:
        print("Too low")
    else:
        print("Too high")


